<odoo>
    <data noupdate="0">
        <record id="product_category_inherit_form" model="ir.ui.view">
            <field name="name">product.category.inherit.form</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="product.product_category_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='first']" position="after">
                    <group>
                        <field name="sec" string="Secuencia"/>
                        <field name="sequence_subgroups_id" string="Secuencia sub-categoria"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>

</odoo>